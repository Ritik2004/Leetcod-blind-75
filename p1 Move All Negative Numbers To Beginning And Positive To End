public class Solution {
    public static int[] separateNegativeAndPositive(int a[]) {
            int n = a.length;
             int ans[] = new int[n];
             int i=0;
             int j = n-1;
             while(i<j){
                 if(a[i]>0 && a[j]<0){
                   int temp = a[i];
                   a[i] = a[j];
â€¦             }
             return a;
    }
}
